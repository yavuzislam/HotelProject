@model CreateRoomDto

@{
    ViewData["Title"] = "CreateRoom";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}
<form method="post">
    <label>RoomNumber</label>
    <input type="text" asp-for="RoomNumber" class="form-control" />
    <br />
    <label>Title</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br />
    <label>Price</label>
    <input type="text" asp-for="Price" class="form-control" />
    <br />
    <label>BedCount</label>
    <input type="text" asp-for="BedCount" class="form-control" />
    <br />
    <label>BathCount</label>
    <input type="text" asp-for="BathCount" class="form-control" />
    <br />
    <label>RoomCoverImage</label>
    <input type="text" asp-for="RoomCoverImage" class="form-control" />
    <br />
    <label>Description</label>
    <input type="text" asp-for="Description" class="form-control" />
    <br />
    <label>Wifi</label>
    <input type="text" asp-for="Wifi" class="form-control" />
    <br />
    <button class="btn btn-primary">Room Ekle</button>
</form>